{% extends "_layouts/base" %}
{% block body_class %}view-guarantee{% endblock %}

{% block sitemeta %}
<title>{{ "MavenShake - No-nonsense Nutrition"|t }}</title>
<meta name="description" content="{{ "The example templates for Craft Commerce"|t }}">
{% endblock %}

{% block main %}
  <div class="message-main">
    <h1 class="heading heading--page">{{ entry.headline }}</h1>
    
    {% if entry.subhead %}
      <p class="subhead">
        {{ entry.subhead }}
      </p>
    {% endif %}
  </div>
{% endblock %}